# NoneBot2 配置
# https://v2.nonebot.dev/docs/tutorial/configuration
DRIVER=~fastapi+~httpx+~websockets
HOST=0.0.0.0
PORT=8080
NICKNAME=[]
SUPERUSERS=[]
COMMAND_START=["/", "／"]
COMMAND_SEP=[".","。"]

# OneBot 配置
# https://onebot.adapters.nonebot.dev/docs/guide/configuration
ONEBOT_ACCESS_TOKEN
ONEBOT_V12_ACCESS_TOKEN

# Sentry
SENTRY_ENVIRONMENT=prod
SENTRY_DSN

# QQ 频道
# https://bot.q.qq.com/wiki/#
QQGUILD_IS_SANDBOX=false
QQGUILD_BOTS=[]

# 数据存储
# https://github.com/he0119/nonebot-plugin-datastore
DATASTORE_DATA_DIR=./data
DATASTORE_CONFIG_DIR=./data/config
DATASTORE_CACHE_DIR=./data/cache
DATASTORE_DATABASE_ECHO=false
DATASTORE_DATABASE_URL

# Nonebot-bison
# https://nonebot-bison.vercel.app/usage/#%E9%85%8D%E7%BD%AE
# https://github.com/felinae98/nonebot-bison/blob/main/src/plugins/nonebot_bison/plugin_config.py
BISON_SKIP_BROWSER_CHECK=true
BISON_CONFIG_PATH=./data/bison
BISON_USE_PIC=false
BISON_OUTER_URL=http://localhost:8080/bison
BISON_USE_PIC_MERGE=0
bison_to_me=false

# 词云
# https://github.com/he0119/nonebot-plugin-wordcloud
WORDCLOUD_COLORMAP=viridis
WORDCLOUD_BACKGROUND_COLOR=black
WORDCLOUD_STOPWORDS_PATH
WORDCLOUD_USERDICT_PATH
WORDCLOUD_TIMEZONE

# Teydacore(telegram bot)
# https://github.com/teyda/teydacore
TELEGRAM_TOKEN=TELEGRAM_TOKEN
ONEBOT_WSR_URL=ws://coolqbot:8080/onebot/v12
ONEBOT_WSR_ACCESS_TOKEN=ACCESS_TOKEN

# chatrecorder 迁移所需的配置
# https://github.com/noneplugin/nonebot-plugin-chatrecorder#chatrecorder_record_migration_bot_id
CHATRECORDER_RECORD_MIGRATION_BOT_ID

# nonebot_plugin_naturel_gpt
# https://github.com/KroMiose/nonebot_plugin_naturel_gpt
NG_CONFIG_PATH=./data/config/naturel_gpt.yml

# FF14
FFLOGS_RANGE=14
FFLOGS_CACHE_TIME=4:30:00

# 早安
MORNING_TIME=7:30:00

# 天气
HEWEATHER_KEY

# 复读
REPEAT_MIGRATION_GROUP_ID

# 机器人
# 不过现在有 OpenAI 的 GPT-3，大概这个没啥用了。
TENCENT_AI_SECRET_ID
TENCENT_AI_SECRET_KEY
